{
  "presets": [
    "flow",
    "es2015",
    "stage-0",
    "react"
  ],
  "plugins": [
    [
      "transform-decorators-legacy"
    ],
    [
      "transform-es3-member-expression-literals"
    ],
    [
      "transform-es3-property-literals"
    ],
    [
      "transform-object-assign"
    ],
    [
      "transform-react-remove-prop-types"
    ]
  ]
}